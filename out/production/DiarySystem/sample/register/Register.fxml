      <PasswordField fx:id="registPassword" layoutX="223.0" layoutY="166.0" prefHeight="27.0" prefWidth="230.0" promptText="包含数字，字母和特殊符号" />
      <PasswordField fx:id="registConfirmPassword" layoutX="223.0" layoutY="208.0" prefHeight="27.0" prefWidth="230.0" promptText="确认密码" />
      <TextField fx:id="probAnswer" layoutX="223.0" layoutY="294.0" prefHeight="27.0" prefWidth="230.0" promptText="验证码答案" />
      <Label layoutX="120" layoutY="126.0" text="用户名">
