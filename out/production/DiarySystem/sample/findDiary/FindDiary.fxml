<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.image.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="550.0" prefWidth="860.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.findDiary.FindDiaryController">
   <children>
      <Label layoutX="340.0" layoutY="40.0" text="Find Diary">
         <font>
            <Font name="Monaco" size="28.0" />
         </font>
      </Label>
      <PasswordField layoutX="550.0" layoutY="60.0" prefHeight="27.0" prefWidth="200.0" promptText="Input title for searching" />
      <ImageView fx:id="searchIcon" fitHeight="32.0" fitWidth="32.0" layoutX="780.0" layoutY="60.0">
         <image>
            <Image url="@../../../images/search.png" />
         </image>
      </ImageView>
      <ImageView fx:id="findBackIcon" fitHeight="32.0" fitWidth="32.0" layoutX="64.0" layoutY="44.0" onMouseClicked="#handleFindBackIconButton">
         <image>
            <Image url="@../../../images/back.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
